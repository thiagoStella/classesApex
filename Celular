public class Celular {
    private String marca;
    private String modelo;
	private Integer polegadas;
    private Boolean telaInfinita;
    private Integer camera;
    private Integer bateria;
    Private Decimal preco;       


public Celular(){}

public Celular (String marca, String modelo, Decimal preco){
    this.marca= marca;
    this.modelo = modelo;
    this.preco = preco;    
}
    

 	public String getMarca(){
        return marca;
    }
    public void setMarca(String marca){
        this.marca = marca;
    } 	
	public String getModelo(){
        return modelo;
    }
    public void setModelo(String modelo){
        this.modelo = modelo;
    }
	public Integer getPolegadas (){
        return polegadas;
    }
    public void setPolegadas(Integer polegadas){
        this.polegadas = polegadas;
    }
	public Boolean getTeleInfinita (){
        return telaInfinita;
    }
    public void setTelaInfinita(String teleInfinita){
        this.telaInfinita = telaInfinita;
    }
	    }
	public Integer getCamera (){
        return camera;
    }
    public void setCamera(String camera){
        this.camera = camera;
    }
	public Boolean getBateria ()){
        return bateria;
    }
    public void setBateria(String bateria){
        this.bateria = bateria;
    }
	public Deciman getPreco ()){
        return preco;
    }
    public void setPreco(String modelo){
        if (preco > 0) {
       		this.preco = preco;
        }else{
            this.preco = null
        	
        }
    }

}
